{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Flutter Web (Custom Port)",
        "type": "dart",
        "request": "launch",
        "program": "lib/main.dart",
        "args": [
          "--web-hostname=127.0.0.1",
          "--web-port=9000",
          "--web-browser-flag=--disable-web-security"
        ],
        "noDebug": false,
      },
    ]
  }